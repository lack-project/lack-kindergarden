{
    "name": "lack/kindergarden",
    "type": "project",
    "description": "Ki Kit",
    "keywords": [],
    "homepage": "https://infracamp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Leuffen",
            "email": "matthes@leuffen.de"
        }
    ],
    "require": {
        "php": ">=8.3",
        "phore/core": "^1.1.1",
        "ext-curl": "*",
        "lack/keystore": "^1.0.0",
        "phore/hydrator": "*"
    },
    "bin": [
        "bin/kg"
    ],

    "suggest": {
        "lack/keystore": "For key-value storage"
    },
    "require-dev": {
        "phore/tester": "*"

    },
    "autoload": {
        "psr-4": {
            "Lack\\Kindergarden\\": "src"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "source",
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
